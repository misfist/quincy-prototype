{
  "name": "quincy",
  "version": "1.0.0",
  "description": "Custom WordPress theme.",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "build": "webpack --mode=production",
    "dev": "webpack --mode=development --watch --quiet",
    "eslint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "eslint:fix": "eslint \"src/**/*.{js,jsx}\" --quiet --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,scss}\"",
    "stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss}\" --fix",
    "wp-pot": "wp-pot --src '*.php' --dest-file 'languages/_s.pot' --domain 'quincy'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carrieforde/_s-with-webpack.git"
  },
  "keywords": [
    "wordpress",
    "webpack"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@wordpress/prettier-config": "^1.1.0",
    "@wordpress/scripts": "^17.1.0",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.2.1",
    "browser-sync": "^2.26.13",
    "browser-sync-webpack-plugin": "^2.3.0",
    "css-loader": "^3.6.0",
    "css-mqpacker": "^7.0.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-prettier": "^3.3.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^4.3.0",
    "image-minimizer-webpack-plugin": "^1.0.0",
    "imagemin": "^5.3.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-pngquant": "^9.0.1",
    "imagemin-svgo": "^8.0.0",
    "img-loader": "^3.0.2",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
